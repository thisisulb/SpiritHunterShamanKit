IF
	HPLT(Myself,2)
	Global("UB_SHDEAD","GLOBAL",0)
THEN
	RESPONSE #100
		SetInterrupt(FALSE)
		SetGlobal("UB_SHDEAD","GLOBAL",1)
		ReallyForceSpellRES("UB_SH0D",Myself)  // <NO TEXT>
		SmallWait(5)
		DestroySelf()
		SetInterrupt(TRUE)
END

IF
	GlobalLT("UB_SHLV","GLOBAL",2)
	OR(2)
		Level(Player1,2)
		LevelGT(Player1,2)
THEN
	RESPONSE #100
		SetInterrupt(FALSE)
		ReallyForceSpellRES("UB_SH0C",Myself)  // No such index
		SetGlobal("UB_SHLV","GLOBAL",2)
		DisplayString(Myself,17119)  // Level Up
		PlaySound("GAM_11")
		SmallWait(10)
		SetInterrupt(TRUE)
END

